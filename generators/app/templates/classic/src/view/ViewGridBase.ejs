Ext.define('Thewall.view.<%= xtypePlural %>.<%= XtypeCapitalizedPlural %>GridBase', {
  extend: 'Thewall.userux.grid.GridBase',

  mixins: ['Thewall.view.<%= xtypePlural %>.GridColumnsCfg'],

  requires: [
    'Thewall.view.<%= xtypePlural %>.ViewCtrl<%= XtypeCapitalizedPlural %>GridBase'
  ],
  controller: '<%= xtypePlural %>gridbase',

  layoutColumns: function(cols) {
    var me = this;

    me.columns = [
      cols.<%= idPropertyEdp %>,
      cols.title,
      cols.is_hidden
    ];
  },
});
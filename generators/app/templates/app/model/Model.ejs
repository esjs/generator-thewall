  extend: 'Thewall.model.common.grid.ModelGridBase',
  idProperty: '<%= idPropertyEdp %>',

  requires: [
    'Thewall.proxy.<%= xtypePlural %>.Proxy<%= XtypeCapitalizedPlural %>'
  ],
  proxy: {
    type: '<%= xtypePlural %>'
  },

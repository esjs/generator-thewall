    {name: '<%= idPropertyEdp %>'},
    <% if (!edpOnly) { %>{name: '<%= idProperty %>'},<% } %>
    {name: 'alias'},
    {name: 'title'},
    {name: 'is_hidden', defaultValue: '0'}
  ],
  validators: {
    title: 'presence'
  }
